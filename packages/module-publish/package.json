{
  "name": "@zwd/z-ui-module-publish",
  "version": "2.5.6-rc.0",
  "scripts": {
    "dev:all": "vite",
    "build:all": "vue-tsc --noEmit && vite build && npm run module:vue && npm run module:vuetify",
    "build:zui": "node build.js",
    "module:vue": "vite build -c vite.module.vue.ts",
    "module:vuetify": "vite build -c vite.module.vuetify.ts",
    "module:zui": "vite build -c vite.module.zui.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "2.6.14",
    "vuetify": "2.5.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2",
    "vite-plugin-vue2": "^1.8.1",
    "vue-template-compiler": "^2.6.14"
  }
}
