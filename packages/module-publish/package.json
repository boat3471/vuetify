{
  "private": true,
  "name": "@zwd/z-ui-module-publish",
  "version": "2.5.816-beta.1",
  "scripts": {
    "dev:all": "vite",
    "build:module": "rimraf dist vue-tsc --noEmit && npm run module:vue && npm run module:zui && npm run module:minio",
    "module:vue": "vite build -c vite.module.vue.ts",
    "module:zui": "node scripts/build.js && vite build -c vite.module.zui.ts",
    "module:minio": "node scripts/minio.js",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-plugin-vue2": "^1.8.1",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.2.2",
    "rimraf": "^3.0.2",
    "minio": "^7.0.18"
  }
}